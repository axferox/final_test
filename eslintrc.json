{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-undef": "off",
    "comma-dangle": "off",
    "implicit-arrow-linebreak": "off",
    "no-console": "error",
    "semi": ["error", "always"],
    "quotes": ["error", "single"]
  }
}
